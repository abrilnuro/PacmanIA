<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/pacman_banner_five"
    android:gravity="center|bottom"
    tools:context="com.aimewexample.pacman.HomeActivity">

    <Button
        android:id="@+id/button_home_profundidad"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@drawable/button_home_background"
        android:text="Profundidad"
        android:textSize="20sp"/>

    <Button
        android:id="@+id/button_home_asterisco"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:background="@drawable/button_home_background"
        android:layout_below="@+id/button_home_profundidad"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="40dp"
        android:textSize="20sp"
        android:text="A*" />



</RelativeLayout>
